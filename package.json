{
	"name": "udapp",
	"version": "0.1.0",
	"private": true,
	"main": "public/electron.js",
	"author": "Aliosha Rabeshko",
	"homepage": "./",
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"docxtemplater": "^3.18.0",
		"electron-is-dev": "^1.2.0",
		"electron-squirrel-startup": "^1.0.0",
		"node-sass": "^4.14.1",
		"pizzip": "^3.0.6",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.1",
		"reactjs-popup": "^1.5.0",
		"sequelize": "^6.3.4",
		"sqlite3": "^5.0.0"
	},
	"scripts": {
		"start2": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"postinstall": "electron-builder install-app-deps",
		"pack": "electron-builder --dir",
		"pack-all": "electron-builder -w",
		"electron-pack": "build --em.main=build/electron.js",
		"preelectron-pack": "npm build",
		"start": "concurrently \"BROWSER=none npm run start2\" \"wait-on http://localhost:3000 && electron .\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"electron": "^9.2.0",
		"electron-builder": "^22.8.0",
		"electron-prebuilt": "^1.4.13",
		"wait-on": "^5.1.0"
	},
	"build": {
		"appId": "com.example.udapp",
		"files": [
			"build/**/*",
			"node_modules/**/*",
			"package.json"
		],
		"directories": {
			"buildResources": "assets"
		}
	}
}
